{
  "name": "covid-hexaland-api",
  "version": "1.0.0",
  "licenses": [
    {
      "type": "GNU General Public License v3.0",
      "url": "https://github.com/FreakPirate/covid-hexaland/raw/master/LICENSE"
    }
  ],
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist",
    "start:dev": "nodemon --config nodemon.json",
    "start:dev:debug": "nodemon --config nodemon.debug.json",
    "test": "nodemon --config nodemon.test.json",
    "heroku-postbuild": "cd client && yarn install && yarn build"
  },
  "_moduleAliases": {
    "@server": "dist/server",
    "@shared": "dist/shared",
    "@constants": "dist/constants",
    "@routes": "dist/routes",
    "@models": "dist/models",
    "@typings": "dist/typings",
    "@validations": "dist/validations"
  },
  "dependencies": {
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fastest-validator": "^1.5.1",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "sprintf-js": "^1.1.2",
    "winston": "^3.3.3",
    "fs-extra": "^9.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.22",
    "@types/sprintf-js": "^1.1.2",
    "@types/supertest": "^2.0.10"
  }
}
